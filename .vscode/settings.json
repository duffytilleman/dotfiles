{
    "debug.javascript.usePreview": false,
    "terminal.integrated.scrollback": 10000,
    "workbench.tree.indent": 16,
    "workbench.editor.labelFormat": "short",
    "sql-formatter.uppercase": true,
    "vim.leader": ",",
    "vim.insertModeKeyBindings": [
        {
          "before": ["k", "j"],
          "after": ["<Esc>"]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [ "<Space>" ],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": ["<leader>", "c", "<Space>"],
            "commands": [
                "editor.action.commentLine"
            ]
        },
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["<C-o>"],
            "commands": [
                "workbench.action.navigateBack"
            ]
        },
        {
            "before": ["<C-p>"],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        {
            "before": ["<tab>"],
            "commands": [
                "workbench.action.navigateForward"
            ]
        },
        {
            "before": [ "<Space>" ],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": ["<leader>", "c", "<Space>"],
            "commands": [
                "editor.action.commentLine"
            ]
        },
        {
            "before": ["<leader>", "e", "v"],
            "commands": [
                "workbench.action.openSettingsJson"
            ]
        },
        {
            "before": ["<leader>", "z", "a"],
            "commands": [
                "editor.toggleFold"
            ]
        },
        {
            "before": ["g", "h"],
            "commands": [
                "workbench.action.focusLeftGroupWithoutWrap"
            ]
        },
        {
            "before": ["g", "l"],
            "commands": [
                "workbench.action.focusRightGroupWithoutWrap"
            ]
        },
        {
            "before": ["g", "j"],
            "commands": [
                "workbench.action.focusBelowGroupWithoutWrap"
            ]
        },
        {
            "before": ["g", "k"],
            "commands": [
                "workbench.action.focusAboveGroupWithoutWrap"
            ]
        },
        {
            "before": ["<return>"],
            "after": [ "o", "<esc>" ]
        },
    ],
    "git.suggestSmartCommit": false,
    "vs-kubernetes": {
        "vs-kubernetes.knownKubeconfigs": [
            "/Users/duffy/.kube/config"
        ],
        "vs-kubernetes.kubeconfig": "/Users/duffy/.kube/config"
    },
    "[javascript]": {
        "editor.tabSize": 2
    },
    "workbench.colorTheme": "Visual Studio Light",
    "editor.renderWhitespace": "boundary",
    "editor.tabSize": 2,
    "files.insertFinalNewline": true,
    "gitlens.remotes": [
    ],
    "gitlens.heatmap.toggleMode": "window",
    "gitlens.codeLens.enabled": false,
    "tabnine.experimentalAutoImports": true,
    "extensions.ignoreRecommendations": false,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "vue"
    ],
    "vetur.experimental.templateInterpolationService": true,
    "explorer.confirmDelete": false,
    "editor.quickSuggestionsDelay": 100,
    "[sql]": {
        "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
    },
    "editor.accessibilitySupport": "off",
    "files.associations": {
        ".gmrc": "jsonc"
    },
    "pgsql.connections": [
        {
            "host": "localhost",
            "dbname": "app",
            "user": "postgres",
            "password": "",
            "emptyPasswordInput": false,
            "port": "5432",
            "profileName": "dev"
        },
        {
            "host": "localhost",
            "dbname": "app",
            "user": "postgres",
            "password": "",
            "emptyPasswordInput": false,
            "port": "5432",
            "profileName": "local-pim",
            "connectTimeout": 15,
            "encrypt": false,
            "applicationName": "vscode-pgsql",
            "hostaddr": "",
            "options": "",
            "sslmode": "prefer",
            "clientEncoding": "",
            "sslcompression": true,
            "sslcert": "",
            "sslkey": "",
            "sslrootcert": "",
            "sslcrl": "",
            "requirepeer": ""
        }
    ],
    "typescript.updateImportsOnFileMove.enabled": "always",
}
